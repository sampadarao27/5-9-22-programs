#include<stdio.h>
#include<stdlib.h>

 void arrayelements(int a[],int n,int j)
 {   
     if(j>=n)
     return ;
     
     
     printf("%d\n",a[j]);

      arrayelements(a,n,j+1); 

 }
 void main()
 { 
    int i,n,j=0;
    int a[100];
    printf("enter your range");
    scanf("%d",&n);


     for(i=0;i<n;i++)
     {
         scanf("%d",&a[i]);
     }
     arrayelements(a,n,j);
 }
